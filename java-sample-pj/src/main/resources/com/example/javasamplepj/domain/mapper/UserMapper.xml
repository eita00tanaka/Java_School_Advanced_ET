<?xml version="1.0" encoding="UTF-8" ?>

<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.example.java_sample_pj.domain.mapper.UserMapper">
  <select id="findAll" resultType="com.example.java_sample_pj.domain.model.user.User">
    select
        user_id
      , user_name
      , password
      , mail_address
      , create_date
      , update_date
      , delete_date
    from
      users
  </select>
</mapper>
